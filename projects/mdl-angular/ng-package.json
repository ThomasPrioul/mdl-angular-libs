{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/mdl-angular",
  "assets": [
    {
      "input": "scss",
      "glob": "**/*.scss",
      "output": "scss"
    },
    {
      "input": "assets",
      "glob": "**/*",
      "output": "assets"
    }
  ],
  "lib": {
    "entryFile": "index.ts"
  }
}
